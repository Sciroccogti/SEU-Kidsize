<?xml version="1.0"?>
<launch>
  <!-- start update -->
  <include file="$(find start)/launch/update_params.launch" />
  <!-- start player -->
  <node pkg="player" type="player" name="player" output="screen"/>
  <!-- start motion -->
  <node pkg="motion" type="motion" name="motion" output="screen"/>
  <!-- start camera -->
  <node pkg="vision" type="camera" name="camera" output="screen"/>
  <!-- start image_process -->
  <node pkg="vision" type="image_process" name="imgproc" output="screen"/>
</launch>