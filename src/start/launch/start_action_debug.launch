<?xml version="1.0"?>
<launch>
  <!-- start update -->
  <include file="$(find start)/launch/update_params.launch" />
  <!-- start simulation -->
  <include file="$(find simulation)/launch/simulation.launch" />
  <!-- start motion -->
  <node pkg="motion" type="motion" name="motion" output="screen"/>
  <!-- start debuger -->
  <node pkg="action" type="action_debuger" name="action_debuger" output="screen"/>
</launch>