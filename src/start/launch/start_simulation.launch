<?xml version="1.0"?>
<launch>
  <!-- start update -->
  <include file="$(find start)/launch/update_params.launch" />
  <!-- start simulation -->
  <include file="$(find simulation)/launch/simulation.launch" />
  <!-- start image_process -->
  <node pkg="vision" type="sim_vision" name="vision" output="screen"/>
  <!-- start motion -->
  <node pkg="motion" type="motion" name="motion" output="screen"/>
  <!-- start control -->
  <node pkg="control" type="main.py" name="control" output="screen"/>
</launch>